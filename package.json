{
  "name": "@larapida/web-nodes",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^22.14.0",
    "pnpm": "^10.10.0"
  },
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "prepare": "husky",
    "commit": "cz",
    "clean:node_modules": "pnpm dlx rimraf --glob ./**/node_modules",
    "clean:dist": "pnpm dlx rimraf --glob ./**/dist",
    "clean": "pnpm exec nx reset && pnpm dlx rimraf tmp .nx && pnpm run clean:dist && pnpm run clean:node_modules",
    "reset": "pnpm run clean && pnpm install && pnpm exec nx reset"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.44.0",
    "@react-router/express": "^7.6.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.8.0",
    "@larapida/shared-server-utils": "workspace:^",
    "@nx/eslint": "21.0.3",
    "@nx/eslint-plugin": "21.0.3",
    "@nx/js": "21.0.3",
    "@nx/node": "21.0.3",
    "@nx/web": "21.0.3",
    "@nx/webpack": "21.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "~18.16.9",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "^9.1.7",
    "nx": "21.0.3",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "webpack-cli": "^5.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
